<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.App.Starting" parent="Theme.SplashScreen">
        <item name="android:windowSplashScreenBackground">#FFFFFF</item>

        <item name="android:windowSplashScreenAnimatedIcon">@drawable/baseline_accessibility_24
        </item>
        <item name="postSplashScreenTheme">@style/Theme.Test8</item>
    </style>

    <style name="BottomNavigationView.ActiveTextAppearance" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">@font/spf</item>
    </style>

    <style name="BottomNavigationView.InactiveTextAppearance" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">@font/spf</item>
    </style>
</resources>